<!DOCTYPE web-app
 PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
<filter>
  <filter-name>CorsFilter</filter-name>
  <filter-class>org.apache.catalina.filters.CorsFilter</filter-class>
</filter>
<filter-mapping>
  <filter-name>CorsFilter</filter-name>
  <url-pattern>/*</url-pattern>
</filter-mapping>
 <servlet>
  <servlet-name>StanServlet</servlet-name>
  <servlet-class>StanServlet</servlet-class>
 </servlet>
 
 <servlet-mapping>
        <servlet-name>StanServlet</servlet-name>
        <url-pattern>/StanServlet</url-pattern>
    </servlet-mapping>
	
	 <servlet>
  <servlet-name>CoucheDB</servlet-name>
  <servlet-class>CoucheDB</servlet-class>
 </servlet>
 
 <servlet-mapping>
        <servlet-name>CoucheDB</servlet-name>
        <url-pattern>/CoucheDB</url-pattern>
    </servlet-mapping>
	
	
	 <servlet>
  <servlet-name>DisplayAll</servlet-name>
  <servlet-class>DisplayAll</servlet-class>
 </servlet>
 
 <servlet-mapping>
        <servlet-name>DisplayAll</servlet-name>
        <url-pattern>/DisplayAll</url-pattern>
    </servlet-mapping>
	 <servlet>
  <servlet-name>StanfordAdmin</servlet-name>
  <servlet-class>StanfordAdmin</servlet-class>
 </servlet>
 
 <servlet-mapping>
        <servlet-name>StanfordAdmin</servlet-name>
        <url-pattern>/StanfordAdmin</url-pattern>
    </servlet-mapping>
	
</web-app>